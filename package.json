{
  "name": "client",
  "version": "1.0.0",
  "description": "Client package.json",
  "main": "index.js",
  "scripts": {
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "postinstall": "webpack -p",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "build": "npm run build && aws s3 sync build/ s3://bufollowbucket-react",
    "start": "node server.js",
    "devserver": "live-server",
    "compile:sass": "node-sass ./style/sass/main.scss ./style/css/sassStyle.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" ./style/css/style.comp.css -o ./style/css/style.prefix.css",
    "compress:css": "node-sass ./style/css/style.prefix.css ./style/css/sassStyle.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "watch:sass": "nodemon -e scss -x \"npm run compile:sass\""
  },
  "author": "",
  "license": "ISC",
